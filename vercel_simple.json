{
  "version": 2,
  "builds": [
    {
      "src": "app_simple.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app_simple.py"
    }
  ],
  "env": {
    "PYTHONPATH": "./",
    "PYTHONUNBUFFERED": "1"
  }
}
